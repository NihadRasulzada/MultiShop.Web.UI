@model GetCargoCustomerByIdDto

@{
    ViewData["Title"] = "UserAddressInfo";
    Layout = "~/Areas/Admin/Views/AdminLayout/_AdminLayout.cshtml";
}

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <div class="bs-example" data-example-id="bordered-table">
                    <h3>Kullanıcı Adres ve İletişim Bilgisi</h3>
                    <div class="alert alert-info">Kullanıcı Id: @Model.Id</div>
                    <div class="alert alert-warning">Kullanıcı Adı Soyadı: @(Model?.Name ?? "N/A") @(Model?.Surname ?? "N/A")</div>
                    <div class="alert alert-info">Email Adresi: @(Model?.Email ?? "N/A")</div>
                    <div class="alert alert-warning">Telefon: @(Model?.Phone ?? "N/A")</div>
                    <div class="alert alert-info">İlçe-Şehir: @(Model?.District ?? "N/A") @(Model?.City ?? "N/A")</div>
                    <div class="alert alert-warning">Adres: @(Model?.Address ?? "N/A")</div>
                </div>
            </div>
        </div>
    </div>
</section>